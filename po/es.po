msgid "welcome_header"
msgstr "¡Hola mundo!"

msgid " option %s"
msgstr " opción %s"

msgid " warning : the minimum word size cannot be negative, using \"0\"\n"
msgstr " advertencia: el tamaño mínimo de palabra no puede ser negativo, se usará \"0\"\n"

msgid "%s version %s, gittag %s\n\n"
msgstr "%s versión %s, gittag %s\n\n"

msgid "Usage: %s [-m,--min-word-size n] [string]\n"
msgstr "Uso: %s [-m,--min-word-size n] [cadena]\n"

msgid "       finds all of the permutations of a string that are more that n characters in length.\n"
msgstr "       encuentra todas las permutaciones de una cadena que tienen más de n caracteres de longitud.\n"

msgid "       default for n = 2\n"
msgstr "       predeterminado para n = 2\n"

msgid "       default for string = \"ADOB\"\n"
msgstr "       valor predeterminado para string = \"ADOB\"\n"

msgid " ?? getopt returned character code 0%o ??\n"
msgstr " ?? getopt devolvió el código de carácter 0%o ??\n"

msgid " finding all of the permutations of %s that are more than %i characters long\n"
msgstr " encontrar todas las permutaciones de %s que tengan más de %i caracteres\n"

msgid "Usage: %s [OPTION]... [STRING]\n"
msgstr "Uso: %s [OPCIÓN]... [CADENA]\n"

msgid "Find the permutations of a string.\n"
msgstr "Encuentra las permutaciones de una cadena.\n"

msgid "  -m, --min-word-size=N    the minimum size of a derived word\n"
msgstr "  -m, --min-word-size=N    el tamaño mínimo de una palabra derivada\n"

msgid "  -x, --max-word-size=X    the maximum size of a derived word\n"
msgstr "  -x, --max-word-size=X    el tamaño máximo de una palabra derivada\n"

msgid "  -?, --help               display this help and exit\n"
msgstr "  -?, --help               muestra esta ayuda y sale\n"

msgid "  -v, --version            output version information and exit\n"
msgstr "  -v, --version            mostrar información de la versión y salir\n"

msgid "  STRING                   the string of characters to be permuted\n"
msgstr "  CADENA                   la cadena de caracteres que se permutará\n"

#, fuzzy
msgid "       default for N = 2\n"
msgstr "       predeterminado para n = 2\n"

#, fuzzy
msgid "       default for X = length of STRING\n"
msgstr "       predeterminado para n = 2\n"

#, fuzzy
msgid "       default for STRING = \"ADOB\"\n"
msgstr "       valor predeterminado para string = \"ADOB\"\n"

msgid "       to find the permutations of STRING taken M at a time (i.e., P(STRING,M))\n"
msgstr "       para encontrar las permutaciones de STRING tomadas de M en M (es decir, P(STRING,M))\n"

msgid "       enter: permute -m M -x M STRING\n"
msgstr "       entrar: permuta -m M -x M STRING\n"

msgid " ///// ERROR : The length of your string is %s.\n               The maximum allowed is %s.\n"
msgstr " ///// ERROR : La longitud de su cadena es %s.\n               El máximo permitido es %s.\n"

#, fuzzy
