msgid " option %s"
msgstr " opción %s"

msgid " warning : the minimum word size cannot be negative, using \"0\"\n"
msgstr " advertencia: el tamaño mínimo de palabra no puede ser negativo, se usará \"0\"\n"

msgid "%s version %s, gittag %s\n\n"
msgstr "%s versión %s, gittag %s\n\n"

msgid "Usage: %s [-m,--min-word-size n] [string]\n"
msgstr "Uso: %s [-m,--min-word-size n] [cadena]\n"

msgid "       finds all of the permutations of a string that are more that n characters in length.\n"
msgstr "       encuentra todas las permutaciones de una cadena que tienen más de n caracteres de longitud.\n"

msgid "       default for n = 2\n"
msgstr "       predeterminado para n = 2\n"

msgid "       default for string = \"ADOB\"\n"
msgstr "       valor predeterminado para string = \"ADOB\"\n"

msgid " ?? getopt returned character code 0%o ??\n"
msgstr " ?? getopt devolvió el código de carácter 0%o ??\n"

msgid " finding all of the permutations of %s that are more than %i characters long\n"
msgstr " encontrar todas las permutaciones de %s que tengan más de %i caracteres\n"
