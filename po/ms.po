msgid "welcome_header"
msgstr "Halo dunia!"

msgid " option %s"
msgstr " pilihan %s"

msgid " warning : the minimum word size cannot be negative, using \"0\"\n"
msgstr " amaran : saiz perkataan minimum tidak boleh negatif, menggunakan \"0\"\n"

msgid "%s version %s, gittag %s\n\n"
msgstr "%s versi %s, gittag %s\n\n"

msgid "Usage: %s [-m,--min-word-size n] [string]\n"
msgstr "Penggunaan: %s [-m,--min-word-size n] [string]\n"

msgid "       finds all of the permutations of a string that are more that n characters in length.\n"
msgstr "       mencari semua permutasi bagi satu rentetan yang lebih daripada n aksara panjangnya.\n"

msgid "       default for n = 2\n"
msgstr "       lalai untuk n = 2\n"

msgid "       default for string = \"ADOB\"\n"
msgstr "       lalai untuk string = \"ADOB\"\n"

msgid " ?? getopt returned character code 0%o ??\n"
msgstr " ?? getopt mengembalikan kod watak 0%o ??\n"

msgid " finding all of the permutations of %s that are more than %i characters long\n"
msgstr " mencari semua permutasi %s yang lebih daripada %i aksara panjang\n"

msgid "Usage: %s [OPTION]... [STRING]\n"
msgstr "Penggunaan: %s [PILIHAN]... [RENTETAN]\n"

msgid "Find the permutations of a string.\n"
msgstr "Cari permutasi bagi satu rentetan.\n"

msgid "  -m, --min-word-size=N    the minimum size of a derived word\n"
msgstr "  -m, --min-word-size=N    saiz minimum bagi perkataan terbitan\n"

msgid "  -x, --max-word-size=X    the maximum size of a derived word\n"
msgstr "  -x, --max-word-size=X    saiz maksimum bagi perkataan terbitan\n"

msgid "  -?, --help               display this help and exit\n"
msgstr "  -?, --help               paparkan bantuan ini dan keluar\n"

msgid "  -v, --version            output version information and exit\n"
msgstr "  -v, --version            keluarkan maklumat versi dan keluar\n"

msgid "  STRING                   the string of characters to be permuted\n"
msgstr "  RENTETAN                   rentetan aksara yang akan diubah\n"

#, fuzzy
msgid "       default for N = 2\n"
msgstr "       lalai untuk n = 2\n"

#, fuzzy
msgid "       default for X = length of STRING\n"
msgstr "       lalai untuk n = 2\n"

#, fuzzy
msgid "       default for STRING = \"ADOB\"\n"
msgstr "       lalai untuk string = \"ADOB\"\n"

msgid "       to find the permutations of STRING taken M at a time (i.e., P(STRING,M))\n"
msgstr "       untuk mencari permutasi STRING yang diambil M pada satu masa (iaitu, P(STRING,M))\n"

msgid "       enter: permute -m M -x M STRING\n"
msgstr "       masukkan: permute -m M -x M STRING\n"

msgid " ///// ERROR : The length of your string is %s.\n               The maximum allowed is %s.\n"
msgstr " ///// RALAT : Panjang rentetan anda ialah %s.\n               Panjang maksimum yang dibenarkan ialah %s.\n"

msgid " finding all of the permutations of %s that are more than %lu characters long\n"
msgstr " mencari semua permutasi %s yang lebih daripada %i aksara panjang\n"
